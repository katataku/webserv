# POST Success Content-Length: 64
#curl -v localhost:8080/files/success -X POST --data "aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
# POST Failer Content-Length: 65
#curl -v localhost:8080/files/fail -X POST --data "baaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa"
# POST is not Allowed
#curl -v localhost:8080/new -X POST 
# DELETE 
curl -v localhost:8080/files/success -X DELETE
